                            var map, geocoder;

                            dojo.ready(function () {
                                // create the map
                                map = new esri.Map("viewDiv", {
                                    basemap: "hybrid",
                                    center: [-6.83349609375, 34.025347738147936], // long, lat  2311162.217155
                                    zoom: 8
                                });

                                // add a graphics layer for geocoding results
                                map.addLayer(new esri.layers.GraphicsLayer({
                                    id: "results"
                                }));

                                // create the geocoder
                                geocoder = new esri.dijit.Geocoder({
                                    autoNavigate: false, // do not zoom to best result
                                    maxLocations: 100, // increase number of results returned
                                    map: map,
                                    arcgisGeocoder: {
                                        url: "https://geocode.arcgis.com/arcgis/rest/services/World/GeocodeServer",
                                        name: "Esri World Geocoder",
                                        placeholder: "Rechercher un emplacement",
                                        sourceCountry: "Morocco" // limit search to a country
                                    }
                                }, "search");
                                geocoder.startup();
                                //geocoder.focus();

                                var symbol = new esri.symbol.PictureMarkerSymbol({
                                    "angle": 0,
                                    "xoffset": 0,
                                    "yoffset": 10,
                                    "type": "esriPMS",
                                    "url": "https://static.arcgis.com/images/Symbols/Shapes/BluePin1LargeB.png",
                                    "contentType": "image/png",
                                    "width": 24,
                                    "height": 24
                                });
                                var template = new esri.InfoTemplate("${name}", "${*}");


                                dojo.connect(geocoder, "onFindResults", function (response) {
                                    console.log("find results: ", response);
                                    var l = map.getLayer("results");
                                    l.clear();
                                    map.infoWindow.hide();
                                    // Clear the ul before appending new results
                                    $('.simpleGeocoder').find('.Ulmenu').remove();
                                    var Ulmenu = $('<ul>').addClass('Ulmenu');
                                    // Remove the Ulmenu <ul> element

                                    dojo.forEach(response.results, function (r) {
                                        r.feature.attributes.name = r.name;
                                        r.feature.setSymbol(symbol);
                                        r.feature.setInfoTemplate(template);
                                        l.add(r.feature);

                                        // Create a new li element for each result
                                        var li = document.createElement("li");
                                        li.textContent = r.name;

                                        // Add a click event listener to zoom to the selected position
                                        li.addEventListener("click", function () {
                                            //console.log(r.feature._extent);
                                            var extent = r.feature._extent;

                                            if (extent) {
                                                map.setExtent(extent.expand(1.5));
                                            }
                                        });

                                        // Append the li to the ul
                                        Ulmenu.append(li);


                                    });
                                    $('.simpleGeocoder').append(Ulmenu);

                                });
                            });

